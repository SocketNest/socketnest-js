{
  "name": "socketnest-js",
  "version": "1.0.0",
  "description": "SocketNest JS Client Library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "repository": "git@github.com:SocketNest/socketnest-js.git",
  "author": "realdanielmlozano <daniel@danielmlozano.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "mkdir -p dist && cp src/index.js dist/index.js && cp src/index.js dist/index.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.7.0"
  }
}
